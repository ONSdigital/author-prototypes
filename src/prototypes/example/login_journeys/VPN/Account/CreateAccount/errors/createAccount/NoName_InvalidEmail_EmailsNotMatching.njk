---
group: Examples
title: Example prototype
layout: example/views/Author8col
---
{% from "components/button/_macro.njk" import onsButton %}

{% set form =  {
    "method": "GET",
    "attributes": {
        "name": "myForm",
        "onsubmit":  "return validateForm()"
    }
} %}

{% block preMain %}
{{
    onsBreadcrumbs({
        "ariaLabel": 'Back',
        "itemsList": [
            {
                "url": '/prototypes/example/login_journeys/VPN/Account/',
                "id": "back",
                "text": 'Back',
                "attributes": {
                    "data-attribute": "Example attribute"
                }
            }
        ]
    })
}}
{% endblock %}

{% block main %}
    {% call onsPanel({
        "title": 'There are 3 problems with this page',
        "type": 'error',
        "classes": "ons-u-mb-xl"
}) %}
    {{
        onsList({
            "element": 'ol',
            "itemsList": [
                {
                    "text": 'Enter your full name',
                    "url": '#fullname-error',
                    "variants": "inPageLink"
                },
                {
                    "text": 'Enter an email address in a valid format, for example, name@example.com',
                    "url": '#email-error',
                    "variants": "inPageLink"
                },
                {
                    "text": 'Email addresses must match',
                    "url": '#confirm-email-error',
                    "variants": "inPageLink"
                }

            ]
        })
    }}
{% endcall %}
    <h1 class="ons-u-mb-s"><span class="ons-u-fs-m ons-u-db ons-u-mb-xs ons-u-lighter">Create an account</span>Enter your details</h1>

    {{
        onsInput({
            "id": "fullname",
            "name": "fullname",
            "label": {
                "text": "Full name"
            },
            "error": {
                "id": "fullname-error",
                "text": "Enter your full name"
        }
        })
    }}

    {{
        onsInput({
            "id": "email",
            "name": "email",
            "label": {
                "text": "Email address"
            },
            "error": {
                "id": "email-error",
                "text": "Enter an email address in a valid format, for example, name@example.com"
        }
        })
    }}

    {{
        onsInput({
            "id": "Confirmemail",
            "name": "Confirmemail",
            "label": {
                "text": "Confirm email address"
            },
            "error": {
                "id": "confirm-email-error",
                "text": "Email address must match"
        }

        })
    }}


    {{
        onsButton({
            "text": "Continue",
            "classes": "ons-u-mt-l ons-u-mb-l"
        })
    }}

    <p>If you already have an account, you can <a href="/prototypes/example/login_journeys/VPN/Account/">sign in</a>.</p>
    


{% endblock %}

{% block bodyEnd %}

    <script>
        console.log('hello world');
        var Name = sessionStorage.getItem('Fullname');
        var Email = sessionStorage.getItem('EmailAddress');
        var ConfirmEmail = sessionStorage.getItem('ConfirmedEmailAddress');

        if((Name != "")){
            document.getElementById('fullname').value = Name;

        }
        if((Email != "")){
            document.getElementById('email').value = Email;

        }    
        if((ConfirmEmail != "")){
            document.getElementById('Confirmemail').value = ConfirmEmail;

        }

    </script>


    <script>
        function validateForm(){
            console.log('run');
            let a = document.forms["myForm"]["fullname"].value;
            let b = document.forms["myForm"]["email"].value;
            let c = document.forms["myForm"]["Confirmemail"].value;

            let x = a.trim();
            let y = b.trim();
            let z = c.trim();

            sessionStorage.setItem('Fullname', x);
            sessionStorage.setItem('EmailAddress', y);    
            sessionStorage.setItem('ConfirmedEmailAddress', z);   

            if ((x != "") && (y != "") && (z != "") && (y == z) && (y.indexOf("@") != -1) && (y.includes("@ons.gov.uk") == true)) {
 
