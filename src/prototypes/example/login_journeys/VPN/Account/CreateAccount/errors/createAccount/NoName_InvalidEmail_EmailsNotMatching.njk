                document.forms["myForm"].action = "/prototypes/example/login_journeys/VPN/Account/CreateAccount/errors/createAccount/NoName.html";    
            }
            if ((x == "") && (y != "") && (z != "") && (y != z) && (y.indexOf("@") != -1) && (y.includes("@ons.gov.uk") == false)) {
