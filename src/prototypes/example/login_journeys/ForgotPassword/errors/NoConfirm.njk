---
group: Examples
title: Example prototype
layout: example/views/Author8col
---
{% from "components/button/_macro.njk" import onsButton %}

{% set form =  {
    "method": "GET",
    "attributes": {
        "name": "myForm",
        "onsubmit":  "return validateForm()"
    }
} %}

{% block head %}
<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />

<style>
.material-symbols-rounded {
  font-variation-settings:
  'FILL' 1,
  'wght' 400,
  'GRAD' 0,
  'opsz' 48
}

.material-symbols-rounded{
    color: black;
    background-color: #FAC064;
}
</style>
{% endblock %}

{% block preMain %}

{% endblock %}

{% block main %}
    {% call onsPanel({
        "title": 'There is a problem with this page',
        "type": 'error',
        "classes": "ons-u-mb-xl"
}) %}
    {{
        onsList({
            "element": 'ol',
            "itemsList": [

                {
                    "text": 'Confirm your password',
                    "url": '#confirm-password-error',
                    "variants": "inPageLink"
                }


            ]
        })
    }}
{% endcall %}

    <h1 class="ons-u-mb-s">Reset your password</h1>

    {{
    onsPanel({
        "body": '<p>Your password must include at least 16 characters.</p>'
    })
}}

    {{
    onsPassword({
        "id": "password",
        "name": "password",
        "label": {
            "text": "Password"
        },
        "showPasswordText": "Show password"

    })
}}

    {{
    onsPassword({
        "id": "password",
        "name": "password",
        "label": {
            "text": "Confirm password"
        },
        "showPasswordText": "Show password",
        "error": {
            "id": "confirm-password-error",
            "text": "Confirm your password"
        }
    })
}}

    {{
        onsButton({
            "text": "Reset password",
            "classes": "ons-u-mt-l ons-u-mb-l"
        })
    }}



{% endblock %}

{% block bodyEnd %}


{% endblock %}
