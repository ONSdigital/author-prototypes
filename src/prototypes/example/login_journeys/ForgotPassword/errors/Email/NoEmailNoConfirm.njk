---
group: Examples
title: Example prototype
layout: example/views/Author8col
---
{% from "components/button/_macro.njk" import onsButton %}

{% set form =  {
    "method": "GET",
    "attributes": {
        "name": "myForm",
        "onsubmit":  "return validateForm()"
    }
} %}

{% block head %}
<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />

<style>
.material-symbols-rounded {
  font-variation-settings:
  'FILL' 1,
  'wght' 400,
  'GRAD' 0,
  'opsz' 48
}

.material-symbols-rounded{
    color: black;
    background-color: #FAC064;
}
</style>
{% endblock %}

{% block preMain %}
{{
    onsBreadcrumbs({
        "ariaLabel": 'Back',
        "itemsList": [
            {
                "url": '/prototypes/example/login_journeys/',
                "id": "back",
                "text": 'Back',
                "attributes": {
                    "data-attribute": "Example attribute"
                }
            }
        ]
    })
}}
{% endblock %}

{% block main %}
    {% call onsPanel({
        "title": 'There are 2 problems with this page',
        "type": 'error',
        "classes": "ons-u-mb-xl"
}) %}
    {{
        onsList({
            "element": 'ol',
            "itemsList": [

                {
                    "text": 'Enter your email address',
                    "url": '#email-error',
                    "variants": "inPageLink"
                },
                {
                    "text": 'Confirm your email address',
                    "url": '#confirm-email-error',
                    "variants": "inPageLink"
                }


            ]
        })
    }}
{% endcall %}
    <h1 class="ons-u-mb-s">Forgot password?</h1>

    <p>If there is an account associated with the email address provided, you will receive a link to reset your password.</p>

    {{
        onsInput({
            "id": "email",
            "name": "email",
            "label": {
                "text": "Email address"
            },
            "error": {
            "id": "email-error",
            "text": "Enter your email address"
        }
        })
    }}

    {{
        onsInput({
            "id": "Confirmemail",
            "name": "Confirmemail",
            "label": {
                "text": "Confirm email address"
            },
            "error": {
            "id": "confirm-email-error",
            "text": "Confirm your email address"
        }
        })
    }}


    {{
        onsButton({
            "text": "Continue",
            "classes": "ons-u-mt-l ons-u-mb-l"
        })
    }}

    <p>If you already have an account, you can <a href="/prototypes/example/login_journeys/">sign in</a></p>
    


{% endblock %}

{% block bodyEnd %}

{% endblock %}