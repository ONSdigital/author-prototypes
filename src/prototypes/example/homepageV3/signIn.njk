---
group: Examples
title: Example prototype
layout: example/views/Author8col
---
{% from "components/button/_macro.njk" import onsButton %}

{% set form =  {
    "method": "GET",
    "attributes": {
        "name": "myForm",
        "onsubmit":  "return validateForm()"
    }
} %}

{% block main %}
    <h1 class="ons-u-mb-s">Sign in</h1>

    {{
        onsInput({
            "id": "email",
            "name": "email",
            "label": {
                "text": "Email address"
            }
        })
    }}

    {{
    onsPassword({
        "id": "password",
        "name": "password",
        "label": {
            "text": "Password"
        },
        "showPasswordText": "Show password"
    })
}}

    <a class="blockLink ons-u-mb-m  ons-u-mt-m" href="#">Forgot password?</a>


    {{
    onsCheckboxes({
        "checkboxesLabel": "",
        "dontWrap": true,
        "name": "types",
        "borderless": true,
        "checkboxes": [
            {
                "id": "booklet",
                "label": {
                    "text": "Stay signed in"
                },
                "value": "staySignedIn"
            }
        ]
    })
}}


    {{
        onsButton({
            "text": "Sign in",
            "classes": "ons-u-mt-l ons-u-mb-xl"
        })
    }}

    <h2 class="ons-u-mb-m">Need to create an account?</h2>
    <a href="#">Create an account</a>


{% endblock %}

{% block bodyEnd %}
    <script>

    document.getElementById('email').value = 'rachel.goodland@ons.gov.uk';
    document.getElementById('password').value = 'YourPassword';

        function validateForm(){
            document.forms["myForm"].action = "/prototypes/example/homepageV3/";    
            
        }
    </script>
{% endblock %}
